<launch>
    <include file="$(find kinova_bringup)/launch/kinova_robot.launch" />
    <node name="joy_arm" pkg="joy" type="joy_node">
        <param name="dev" value="/dev/input/by-id/usb-Logitech_Wireless_Gamepad_F710_38B492D1-joystick" />
        <param name="deadzone" value="0.1" />
        <param name="autorepeat_rate" value="20.0" />
        <remap from="/joy" to="/joy_arm"/>
    </node>
    <node name="arm_control_marmotte" pkg="arm_control_marmotte" type="arm_control_node.py" output="screen"/>
</launch>